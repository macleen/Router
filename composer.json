        "illuminate/support": "^9.48"
    },
    "license": "MIT",
    "authors": [{
        "name": "Mac Leen",
        "email": "avfoxltd@gmail.com"
    }],
    "minimum-stability": "stable",
    "scripts": {
    "start-server": "php -S localhost:9000 -t ./"
